{
    "name": "automation-api-cypress",
    "version": "1.0.0",
    "description": "Projeto consiste em automatizar testes API no Framework Cypress.",
    "main": "index.js",
    "scripts": {
        "test": "cypress run",
        "cy:open": "cypress open",
        "lint": "eslint cypress/**/*.js",
        "lint:fix": "eslint cypress/**/*.js --fix",
        "cy:open:prd": "cypress open --env configFile=prd",
        "cy:open:dev": "cypress open --env configFile=dev",
        "cy:open:stage": "cypress open --env configFile=stage",
        "cy:test:prd": "cypress run --env configFile=prd",
        "cy:test:dev": "cypress run --env configFile=dev",
        "cy:test:stage": "cypress run --env configFile=stage",
        "cy:allure:ci": "allure generate --clean",
        "cy:allure:report": "allure generate --clean && allure open"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fabiojfadsg/automation-api-cypress.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/fabiojfadsg/automation-api-cypress/issues"
    },
    "homepage": "https://github.com/fabiojfadsg/automation-api-cypress#readme",
    "devDependencies": {
        "@faker-js/faker": "^7.5.0",
        "@shelex/cypress-allure-plugin": "^2.40.2",
        "@testing-library/cypress": "^8.0.3",
        "ajv": "^8.11.0",
        "allure-commandline": "^2.29.0",
        "cpf": "^2.0.1",
        "cypress": "^10.10.0",
        "cypress-file-upload": "^5.0.8",
        "cypress-grep": "^2.6.1",
        "cypress-iframe": "^1.0.1",
        "cypress-mochawesome-reporter": "^3.2.2",
        "cypress-multi-reporters": "^1.6.1",
        "cypress-terminal-report": "^4.1.2",
        "cypress-xpath": "^2.0.1",
        "eslint": "^8.23.0",
        "eslint-plugin-cypress": "^2.12.1",
        "joi": "^17.6.0",
        "mocha-junit-reporter": "^2.0.2"
    },
    "dependencies": {
        "cypress-plugin-api": "^1.3.0"
    }
}
